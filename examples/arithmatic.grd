<start>[A]{OUTPUT("Incrementing cell0")}

<A>[B]{INCREMENT(cell0)}

<G>[H]{SET(cell1, 23)}

<H>[I]{OUTPUT("New value for cell1: {cell1}")}

<I>[J|K]{CHECK(cell1 == 23)}

<J>[end]{OUTPUT("cell1 is equal to set value")}

<K>[end]{OUTPUT("failed to set value in cell1")}

<B>[C]{OUTPUT("Value: {cell0}")}

<C>[D|E]{CHECK(cell0 > 0)}

<D>[F]{OUTPUT("cell0 incremented successfully")}

<E>[end]{OUTPUT("failed to increment cell0")}

<F>[G]{DECREMENT(cell0)}

<end>[]{}
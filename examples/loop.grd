[START {
    print("This is an example program where we count from $[COUNTVAR] to $[LIMITVAR]")
} -> CHECK]

[COUNTVAR {
    1
} -> NONE]

[LIMITVAR {
    10
} -> NONE]

[CHECK {
    if ($[COUNTVAR] <= $[LIMITVAR])
} -> END | OUTPUT]

[OUTPUT {
    print("Value: $[COUNTVAR]")
} -> COUNT]

[COUNT {
    set(COUNTVAR, $[COUNTVAR] + 1)
} -> CHECK]

[END {
    print("Program complete")
} -> NONE]